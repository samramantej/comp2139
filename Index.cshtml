@model List<Project>
@{
    ViewData["Title"] = "Projects";
}
<h2>Projects</h2>
@if (Model.Any())
{
    <table class="table">

        <thead>
            <tr>
                <td>Id</td>
                <td>Name</td>
                <td>Description</td>
            </tr>
        </thead>

        <tbody>
            @foreach(var project in Model)
            {
                <tr>
                    <td>
                        @project.ProjectId;
                    </td>
                    <td>
                        @project.Name;
                    </td>
                    <td>
                        @project.Description;
                    </td>
                </tr>
            }
        </tbody>
    </table>
}
else
{
    <p>No projects Data</p>
}
